    <converters:KeyResourceConverter x:Key="KeyToIconConverter">
        <converters:ValueResourceKey Value="Specs" Resource="IconClipboardOutlined" />
        <converters:ValueResourceKey Value="Sources" Resource="IconXml" />
        <converters:ValueResourceKey Value="Runners" Resource="IconPlayOutlinedCollection" />
        <converters:ValueResourceKey Value="Collection" Resource="IconCollectionThemed" />
        <converters:ValueResourceKey Value="Folder" Resource="IconFolderThemed" />
        <converters:ValueResourceKey Value="Spec" Resource="IconSpecThemed" />
    </converters:KeyResourceConverter>

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="IconForegroundBrushDefault" Color="#5F6E7B" />
            <SolidColorBrush x:Key='IconForegroundBrushCollection'>#6C35B2</SolidColorBrush>
            <SolidColorBrush x:Key='IconForegroundBrushFolder'>#A79600</SolidColorBrush>
            <SolidColorBrush x:Key='IconForegroundBrushSpec'>#126A9B</SolidColorBrush>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Dark">
            <SolidColorBrush x:Key="IconForegroundBrushDefault" Color="#b6d4ed" />
            <SolidColorBrush x:Key='IconForegroundBrushCollection'>#A2A0FF</SolidColorBrush>
            <SolidColorBrush x:Key='IconForegroundBrushFolder'>#F4EB9B</SolidColorBrush>
            <SolidColorBrush x:Key='IconForegroundBrushSpec'>#5EC4FD</SolidColorBrush>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>

    <ControlTheme x:Key="IconBase" TargetType="PathIcon">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Foreground" Value="{DynamicResource IconForegroundBrushDefault}" />
        <Setter Property="Height" Value="30" />
        <Setter Property="Width" Value="30" />
        <Setter Property="Margin" Value="0 1 0 0" />
        <Setter Property="Padding" Value="7" />
        <Setter Property="Template">
            <ControlTemplate>
                <Border Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}"
                        Padding="{TemplateBinding Padding}"
                        Height="{TemplateBinding Height}"
                        Width="{TemplateBinding Width}">
                    <Viewbox>
                        <Path Data="{TemplateBinding Data}" Fill="{TemplateBinding Foreground}" Stretch="Uniform" />
                    </Viewbox>
                </Border>
            </ControlTemplate>
        </Setter>
    </ControlTheme>

