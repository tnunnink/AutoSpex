<ResourceDictionary xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:a="http://schemas.actiprosoftware.com/avaloniaui"
        xmlns:components="using:AutoSpex.Client.Components">
    
    <Design.PreviewWith>
        <components:NodeItem />
    </Design.PreviewWith>

    <ControlTheme x:Key="{x:Type components:NodeItem}" TargetType="components:NodeItem">
        <Setter Property="Template">
            <ControlTemplate>
                <TextBlock Text="Templated Control" />
            </ControlTemplate>
        </Setter>
    </ControlTheme>
</ResourceDictionary>
